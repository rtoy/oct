;;; -*- Mode: lisp -*-

(eval-when (:load-toplevel :compile-toplevel)
  (setf *inline-expansion-limit* 1600))

(mk:defsystem qd
  :source-pathname (make-pathname :directory (pathname-directory *load-pathname*))
  :components
  ((:file "qd-package")
   (:file "qd"
	  :depends-on ("qd-package"))
   (:file "qd-const"
	  :depends-on ("qd-package"))
   (:file "qd-fun"
	  :depends-on ("qd")))
)
